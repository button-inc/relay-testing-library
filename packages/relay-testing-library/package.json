{
  "name": "@button-inc/relay-testing-library",
  "version": "0.0.6",
  "description": "A relay component testing helper",
  "author": "Button Inc.",
  "license": "Apache-2.0",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
 ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/button-inc/relay-testing-library.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react-relay": "^14.1.2",
    "babel-jest": "^29.2.1",
    "babel-plugin-relay": "^14.1.0",
    "react-compiler": "^1.0.3",
    "relay-compiler": "^14.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/relay-runtime": "^14.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.1.0",
    "react-scripts": "^5.0.1",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-runtime": "^14.1.0",
    "relay-test-utils": "^14.1.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  }
}
