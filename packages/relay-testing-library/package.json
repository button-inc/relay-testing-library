{
  "name": "@button-inc/relay-testing-library",
  "version": "0.0.7",
  "description": "A relay component testing helper",
  "author": "Button Inc.",
  "license": "Apache-2.0",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/button-inc/relay-testing-library.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.23",
    "@types/react-relay": "^14.1.2",
    "@types/relay-runtime": "^14.1.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-relay": "^1.8.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0",
    "relay-test-utils": "^14.1.0",
    "typescript": "^4.5.5",
    "yarn-deduplicate": "^6.0.0"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  }
}
